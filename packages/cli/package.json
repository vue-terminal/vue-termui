{
  "name": "@vue-termui/cli",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "version": "0.0.14",
  "scripts": {
    "dev": "esmo src/cli.ts dev demo/main.ts",
    "demo:build": "esmo src/cli.ts build demo/main.ts",
    "build": "rimraf dist && rollup -c --environment NODE_ENV:production",
    "build:dev": "rimraf dist && rollup -c -w",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l @vue-termui/cli -r 1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vtui": "./vtui.mjs"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "vtui.mjs"
  ],
  "keywords": [
    "vue",
    "term",
    "ui",
    "cli",
    "vue-termui-cli",
    "vue-termui"
  ],
  "funding": "https://github.com/posva/vue-termui?sponsor=1",
  "license": "MIT",
  "author": "Eduardo San Martin Morote (https://esm.dev)",
  "repository": {
    "type": "git",
    "url": "https://github.com/posva/vue-termui.git",
    "directory": "packages/cli"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": {
    "url": "https://github.com/posva/vue-termui/issues"
  },
  "homepage": "https://github.com/posva/vue-termui#readme",
  "dependencies": {
    "picocolors": "^1.0.0",
    "vite-node": "^0.5.9",
    "ws": "^8.5.0"
  },
  "peerDependencies": {
    "vite": "^2.8.6",
    "vite-plugin-vue-termui": ">=0.0.8",
    "vue-termui": ">=0.0.10"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@types/ws": "^8.5.3",
    "cac": "^6.7.12",
    "esbuild": "^0.14.38",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "pathe": "^0.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-license": "^2.7.0",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-vue-termui": "workspace:*",
    "vue-termui": "workspace:*"
  }
}
